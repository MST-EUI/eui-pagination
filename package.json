{
  "name": "eui-pagination",
  "version": "0.1.0",
  "description": "eui-pagination react component",
  "main": "dist/index.js",
  "scripts": {
    "start": "eui server",
    "dev": "eui server",
    "build": "eui build",
    "test": "jest",
    "coverage": "jest --coverage",
    "lint": "eslint --fix src/*",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "precommit": "lint-staged",
    "commitmsg": "commitlint -E GIT_PARAMS",
    "version": "npm run changelog && git add CHANGELOG.md",
    "postversion": "git push && git push --tags",
    "prepublishOnly": "npm run build"
  },
  "author": "mistong eui team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/MST-EUI/eui-pagination.git"
  },
  "bugs": {
    "url": "https://github.com/MST-EUI/eui-pagination/issues"
  },
  "homepage": "https://github.com/MST-EUI/eui-pagination",
  "keywords": [
    "react",
    "react-component",
    "eui",
    "eui-pagination"
  ],
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|sass|scss)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "devDependencies": {
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-conventional": "^7.0.1",
    "@mistong/eui-cli": "^0.1.6",
    "@mistong/eui-css": "^0.1.2",
    "babel-core": "^6.10.4",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^23.2.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.3.13",
    "css-loader": "^0.26.1",
    "babel-loader": "^6.4.1",
    "conventional-changelog-cli": "^2.0.1",
    "cz-conventional-changelog": "^2.1.0",
    "es3ify-loader": "^0.2.0",
    "es5-shim": "^4.5.10",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jest": "^21.17.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.8.2",
    "file-loader": "^0.9.0",
    "husky": "^0.14.3",
    "jest": "^23.2.0",
    "lodash": "^4.17.10",
    "node-sass": "^4.9.0",
    "lint-staged": "^7.2.0",
    "rc-pagination": "^1.17.0",
    "react": "^0.14.9",
    "react-dom": "^0.14.9",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.13.1",
    "webpack-hot-middleware": "^2.22.3"
  },
  "dependencies": {
    "@mistong/eui-css": "^0.1.5",
    "classnames": "^2.2.6"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add ."
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "subject-case": [
        0
      ]
    }
  }
}
